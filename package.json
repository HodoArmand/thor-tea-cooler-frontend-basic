  "scripts": {
    "test": "test",
    "css-dev": "call npx tailwindcss -i src/css/tea.css -o public/css/tea.css",
    "css-watch": "call npx tailwindcss -i src/css/tea.css -o public/css/tea.css --watch",
    "css-prod": "00_minify-css.bat",
    "js-copy-lib": "cpx ./src/js/lib/*.js  ./public/js/lib",
    "clean-css": "del-cli --force ./public/css",
    "clean-js": "del-cli --force ./public/js",
    "clean": "npm-run-all -s clean-css clean-js",
    "js-dev": "cp -r ./src/js  ./public/",
    "js-watch": "cpx \"src/js/*.js\" \"public/js\" --watch",
    "js-prod": "00_minify-js.bat",
    "dev": "npm-run-all -s clean css-dev js-dev",
    "watch": "npm-run-all -p -r css-watch js-watch",
    "dev-watch": "npm-run-all -s clean js-copy-lib watch",
    "prod": "npm-run-all -s clean js-copy-lib css-prod js-prod"
  },
